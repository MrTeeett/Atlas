{
  "listen": "127.0.0.1:8080",
  "root": "/",
  "base_path": "/",
  "cookie_secure": true,
  "enable_exec": true,
  "enable_firewall": true,
  "enable_admin_actions": true,
  "log_level": "debug",
  "service_name": "atlas.service",
  "fs_sudo": true,
  "fs_users": [
    "*"
