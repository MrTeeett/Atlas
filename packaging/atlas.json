{
  "listen": "127.0.0.1:8080",
  "root": "/",
  "base_path": "/",
  "cookie_secure": true,
  "enable_exec": true,
  "enable_firewall": true,
  "enable_admin_actions": false,
  "service_name": "atlas.service",
  "daemonize": true,
  "log_level": "info",
  "log_file": "/var/lib/atlas/atlas.log",
  "log_stdout": false,
  "fs_sudo": true,
  "fs_users": [
    "*"
  ],
  "master_key_file": "/var/lib/atlas/atlas.master.key",
  "user_db_path": "/var/lib/atlas/atlas.users.db",
  "firewall_db_path": "/var/lib/atlas/atlas.firewall.db"
}
